{
  "name": "cine-power-planner",
  "version": "1.0.2",
  "description": "Browser-based tool for planning professional camera setups powered by V-Mount or B-Mount batteries. It calculates total power consumption, current draw at 14.4 V and 12 V, and estimated battery runtime while checking that the battery can safely deliver the required power.",
  "main": "data.js",
  "scripts": {
    "lint": "eslint .",
    "check-consistency": "node checkConsistency.js",
    "help": "node cliHelp.js",
    "normalize": "node normalizeData.js",
    "unify-ports": "node unifyPorts.js",
    "generate-schema": "node generateSchema.js",
    "test": "npm run lint && npm run check-consistency && npm run test:jest",
    "test:jest": "node --max-old-space-size=3072 ./node_modules/jest/bin/jest.js --runInBand",
    "test:unit": "node --max-old-space-size=1024 ./node_modules/jest/bin/jest.js --runInBand --selectProjects unit",
    "test:data": "node --max-old-space-size=1024 ./node_modules/jest/bin/jest.js --runInBand --selectProjects data",
    "test:dom": "node --max-old-space-size=1536 ./node_modules/jest/bin/jest.js --runInBand --selectProjects dom",
    "test:script": "node --max-old-space-size=3072 ./node_modules/jest/bin/jest.js --runInBand --selectProjects script"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jest-localstorage-mock": "^2.4.26"
  },
  "dependencies": {
    "lz-string": "^1.5.0"
  }
}
